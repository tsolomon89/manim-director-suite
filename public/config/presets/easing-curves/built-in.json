    "formula": "1 - (1 - t) * (1 - t) * (1 - t)",
    "discrete": false
  },
  {
    "id": "ease-in-out",
    "name": "Ease In-Out",
    "type": "cubic",
    "formula": "t < 0.5 ? 4 * t * t * t : 1 - (-2 * t + 2) ^ 3 / 2",
    "discrete": false
  }
