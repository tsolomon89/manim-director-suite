{
  "parameters": {
    "name": {
      "pattern": "^[a-zA-Z_][a-zA-Z0-9_]*$",
      "maxLength": 50
    },
    "expression": {
      "maxLength": 500,
      "forbiddenPatterns": [
        "while\\s*\\(",
        "for\\s*\\(",
        "eval\\s*\\("
      ]
    },
    "domain": {
      "minRange": 0.0001,
      "maxRange": 1000000,
      "singularityWarningThreshold": 0.001
    }
  }
}
